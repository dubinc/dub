TOKEN dub_tinybird_token READ

DESCRIPTION >
	internal pipe for updating lead event


NODE endpoint
SQL >

    %
    SELECT *
    FROM dub_lead_events
    WHERE
        event_id
        = {{
            String(
                eventId,
                "nLvob3FdmsYuI1BI",
                description="The unique event ID.",
                required=True,
            )
        }}
    ORDER BY timestamp DESC

TYPE endpoint