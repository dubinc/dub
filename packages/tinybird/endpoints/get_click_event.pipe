TOKEN dub_tinybird_token READ

NODE endpoint
SQL >

    %
    SELECT *
    FROM dub_click_events_id
    WHERE
        click_id
        = {{
            String(
                clickId,
                'Z4f1TFvkejrp9o0L',
                description="The unique ID for a given click event",
                required=True,
            )
        }}
    ORDER BY timestamp DESC
    LIMIT 1

TYPE endpoint