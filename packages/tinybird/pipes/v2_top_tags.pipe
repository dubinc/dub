    FROM (SELECT link_id, tag_id FROM workspace_links_with_tags) AS tags
    JOIN
        (
            SELECT link_id
            FROM
                dub_lead_events_mv
                {% if defined(workspaceId) or defined(partnerId) or defined(programId) %}
