                {% if defined(workspaceId) %} AND workspace_id = {{ workspaceId }} {% end %}
                {% if defined(programId) %} AND program_id = {{ programId }} {% end %}
                {% if defined(partnerId) %} AND partner_id = {{ partnerId }} {% end %}
