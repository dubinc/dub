DESCRIPTION >
	Get webhook events

TOKEN "get_webhook_events_endpoint_read_6029" READ

NODE endpoint
SQL >

    %
    SELECT *
    FROM dub_webhook_events
    WHERE
        webhook_id
        = {{
            String(
                webhookId,
                'wh_fjs8YfXkgsFL7eF7LOBvbluDb',
                description="The ID of the webhook",
                required=True,
            )
        }}
        ORDER BY timestamp DESC
        limit 100

TYPE endpoint
