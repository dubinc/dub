  "scripts": {
    "dev": "concurrently --kill-others \"pnpm prisma:generate && next dev --port 8888\" \"pnpm prisma:generate && pnpm prisma:studio --browser none\"",
    "build": "pnpm prisma:generate && next build",
    "lint": "next lint",
    "start": "next start",
    "script": "tsx ./scripts/run.ts",
    "test": "pnpm prisma:generate && vitest -no-file-parallelism --bail=1",
    "generate-openapi": "tsx ./scripts/generate-openapi.ts",
    "prisma:generate": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma generate",
    "prisma:push": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma push",
    "prisma:studio": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma studio",
